<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.example.administrator.testproject.Activity.BLineActivity">
    <com.test.view.WaveView
        android:layout_width="150dp"
        android:background="#ffffff"
        android:layout_height="150dp"
        android:visibility="gone"
        android:layout_centerInParent="true" />
    <com.test.view.WaveLoadingView
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:visibility="gone"
        android:layout_alignParentBottom="true"/>
    <com.test.view.WaveProgressView
        android:id="@+id/waveprogressView"
        android:layout_width="200dp"
        android:layout_height="200dp"
        app:maxProgress="100"
        app:progress="50"
        android:layout_centerInParent="true"/>
    <com.test.view.MyWaveView
        android:id="@+id/wave_view"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:visibility="gone"/>

    <com.test.view.TestWaveView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        android:visibility="gone"
        />
    <ImageView
        android:id="@+id/bline_iv"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/home_collect_checked"/>
</RelativeLayout>
